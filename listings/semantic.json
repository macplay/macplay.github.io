    "icon",
    "image",
    "item",
    "label",
    "list",
    "menu",
    "message",
    "nag",
    "popup",
    "progress",
    "reset",
    "segment",
    "sidebar",
    "site",
    "step",
    "sticky",
    "toast",
    "visibility"
